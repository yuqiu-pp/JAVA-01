<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dynamic.source.mapper.UserMapper">
    <resultMap id="BaseResultMap" type="com.dynamic.source.entity.User">
        <result column="id" property="userId" jdbcType="INTEGER"/>
        <result column="name" property="userName" jdbcType="VARCHAR"/>
        <result column="age" property="userAge" jdbcType="INTEGER"/>
    </resultMap>

    <insert id="insert" parameterType="com.dynamic.source.entity.User">
        insert into user (name, age)
        values ( #{userName,jdbcType=VARCHAR},
        #{userAge,jdbcType=INTEGER})
    </insert>

    <select id="selectAll"  resultMap="BaseResultMap">
        select * from user;
    </select>

</mapper>